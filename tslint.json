{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "function-name": false,
    "no-console": false,
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "max-line-length": [true, 120]
  },
  "moduleResolution": "node",
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [".cache", ".vscode", "dist"]
  }
}
